{
  "name": "moviesapi",
  "version": "0.0.0",
  "description": "simple skeleteon app based on imdb movie metadata",
  "author": "raphael abreu <raphael.alex@gmail.com>",
  "company": "aelkz",
  "keywords": [
    "skeleton",
    "node",
    "express",
    "framework",
    "example"
  ],
  "homepage": "https://github.com/aelkz/moviesAPI",
  "repository": {
    "type": "git",
    "url": "https://github.com/aelkz/moviesAPI.git"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.github.com/aelkz/moviesAPI/master/LICENSE"
    }
  ],
  "private": true,
  "scripts": {
    "start:dev": "nodemon -w src --exec \"DEBUG=app babel-node src/run.js\"",
    "start:prod": "nodemon -w src --exec \"DEBUG=app babel-node src/run.js\"",
    "start": "babel-node src/run.js",
    "build:dev": "babel src -s -D -d dist",
    "build:prod": "babel src -s -D -d dist",
    "docker": "docker",
    "lint": "eslint src",
    "mongo:start": "docker run --name realworld-mongo -p 27017:27017 mongo & sleep 5",
    "mongo:stop": "docker stop realworld-mongo && docker rm realworld-mongo",
    "node": "node",
    "rimraf": "rimraf",
    "start:init": "yarn run -s build",
    "test": "newman run ./tests/api-tests.postman.json -e ./tests/env-api-tests.postman.json",
    "stop": "lsof -ti :3000 | xargs kill"
  },
  "main": "dist/run.js",
  "engines": {
    "node": "4.x.x"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "1.18.2",
    "cluster": "^0.7.7",
    "colors": "^1.2.3",
    "compression": "^1.7.2",
    "cookie-parser": "~1.4.3",
    "cors": "2.8.4",
    "csurf": "^1.9.0",
    "debug": "~3.1.0",
    "dotenv": "^5.0.1",
    "ejs": "2.5.9",
    "errorhandler": "1.4.3",
    "express": "^4.16.3",
    "express-async-wrapper": "^0.1.0",
    "express-jwt": "5.3.1",
    "express-session": "1.15.6",
    "express-sslify": "^1.2.0",
    "express-validation": "^1.0.2",
    "express-validator": "^5.1.2",
    "helmet": "^3.12.0",
    "http-errors": "~1.6.2",
    "http-status": "^1.1.0",
    "jade": "~1.11.0",
    "joi": "^13.3.0",
    "jsonwebtoken": "^8.1.0",
    "less-middleware": "~2.2.1",
    "method-override": "2.3.10",
    "methods": "1.1.2",
    "mongoose": "^5.0.17",
    "mongoose-unique-validator": "2.0.1",
    "morgan": "~1.9.0",
    "ncu": "^0.2.1",
    "node-env-configuration": "^1.0.3",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.3",
    "request": "2.85.0",
    "resource-router-middleware": "^0.7.0",
    "underscore": "1.9.0"
  },
  "devDependencies": {
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "mocha": "^5.1.1",
    "newman": "^3.8.2",
    "nodemon": "^1.17.3",
    "supertest": "^3.0.0"
  }
}
